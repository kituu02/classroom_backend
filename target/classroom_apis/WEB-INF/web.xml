<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>slogin</servlet-name>
    <servlet-class>com.example.slogin</servlet-class>    
  </servlet>
  <servlet>
    <servlet-name>sregister</servlet-name>
    <servlet-class>com.example.sregister</servlet-class>    
  </servlet>
  <servlet>
    <servlet-name>tlogin</servlet-name>
    <servlet-class>com.example.tlogin</servlet-class>    
  </servlet>
  <servlet>
    <servlet-name>tregister</servlet-name>
    <servlet-class>com.example.tregister</servlet-class>    
  </servlet>
  

  <servlet-mapping>
    <servlet-name>slogin</servlet-name>
    <url-pattern>/slogin</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>sregister</servlet-name>
    <url-pattern>/sregister</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>tlogin</servlet-name>
    <url-pattern>/tlogin</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>tregister</servlet-name>
    <url-pattern>/tregister</url-pattern>
  </servlet-mapping>


</web-app>
